# $(DEV) database

file "tpgAsynDriver_smrf.db" {
    pattern {   DEVICE,   PORT         }
            {   "$(DEV)",    "tpgPort"    }
    
}

file "tpgDiagnostics.db" {
    pattern {   DEVICE,    PORT   }
            {   "$(DEV)",  "tpgPort" }
}

file "tpgCountTrig.db" {
    pattern {   DEVICE,    PORT,      N    }
            {   "$(DEV)",  "tpgPort", "0"  }
            {   "$(DEV)",  "tpgPort", "1"  }
            {   "$(DEV)",  "tpgPort", "2"  }
            {   "$(DEV)",  "tpgPort", "3"  }
            {   "$(DEV)",  "tpgPort", "4"  }
            {   "$(DEV)",  "tpgPort", "5"  }
            {   "$(DEV)",  "tpgPort", "6"  }
            {   "$(DEV)",  "tpgPort", "7"  }
            {   "$(DEV)",  "tpgPort", "8"  }
            {   "$(DEV)",  "tpgPort", "9"  }
            {   "$(DEV)",  "tpgPort", "10" }
            {   "$(DEV)",  "tpgPort", "11" }
}

