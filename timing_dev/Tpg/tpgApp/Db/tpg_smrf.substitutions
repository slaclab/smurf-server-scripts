}

file "tpgDiag.db" {
    pattern {   DEVICE,  PORT,     MAX_LENGTH }
            { "$(DEV)", "tpgPort",    "20000" }
}


file "tpgFaultHistory.db" {
    pattern {   DEVICE,    PORT,          BUF    }
            {   "$(DEV)",     "tpgPort",     "0"    }
            {   "$(DEV)",     "tpgPort",     "1"    }
            {   "$(DEV)",     "tpgPort",     "2"    }
            {   "$(DEV)",     "tpgPort",     "3"    }
}


