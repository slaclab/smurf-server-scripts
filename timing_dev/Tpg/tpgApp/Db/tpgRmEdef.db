record(longin, "$(DEVICE):$(INST):IDX")
{
    field(PINI, "YES")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT),0) rm$(RM)_index")
    field(SCAN, "I/O Intr")
}

record(mbbo, "$(DEVICE):$(INST):RATEMODE")
{
    field(PINI, "YES")
    field(DTYP, "asynInt32")
    field(OUT, "@asyn($(PORT),0) rm$(RM)_rateMode")
    field(OMSL, "supervisory")
    field(SCAN, "Passive")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(ZRST, "Fixed Rate")
    field(ONST, "AC Rate")
    field(TWST, "Exp Seq")
    info(autosaveFields, "VAL")
}

