record(ai, "$(DEVICE):RATETXCLK")
{
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),0) rateTxClk")
    field(EGU,  "M counts/s")
    field(PREC, "6")
    field(SCAN, "I/O Intr")
}

