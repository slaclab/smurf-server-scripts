record(longin, "$(DEVICE):COUNTTXCLK")
{
   field(DTYP, "asynInt32")
   field(DESC, "TxClockCounter in TimingFrameRx")
   field(INP,  "@asyn($(PORT),0) countTxClk")
   field(SCAN, "I/O Intr")
}

