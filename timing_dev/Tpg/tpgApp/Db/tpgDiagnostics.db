record(longin, "$(DEVICE):COUNTPLL")
{
  field(DTYP, "asynInt32")
  field(DESC, "PLL Change Counter")
  field(INP,  "@asyn($(PORT),0) countPLL")
  field(SCAN, "I/O Intr")
}

