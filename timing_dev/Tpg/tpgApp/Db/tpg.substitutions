file "tpgDiag.db" {
    pattern {   DEVICE,  PORT,     MAX_LENGTH }
            { "$(DEV)", "tpgPort",    "20000" }
}


file "tpgFaultHistory.db" {
    pattern {   DEVICE,    PORT,          BUF    }
