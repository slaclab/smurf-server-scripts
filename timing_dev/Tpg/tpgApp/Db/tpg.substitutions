# $(DEV) database

file "tpgAsynDriver.db" {
    pattern {   DEVICE,   PORT         }
            {   "$(DEV)",    "tpgPort"    }
    
}

file "tpgDiagnostics.db" {
    pattern {   DEVICE,    PORT   }
            {   "$(DEV)",  "tpgPort" }
}

file "tpgCountTrig.db" {
    pattern {   DEVICE,    PORT,      N    }
            {   "$(DEV)",  "tpgPort", "0"  }
            {   "$(DEV)",  "tpgPort", "1"  }
            {   "$(DEV)",  "tpgPort", "2"  }
